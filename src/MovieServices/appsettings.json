{
  "$schema": "https://steeltoe.io/schema/latest/schema.json",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Steeltoe": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Eureka": {
    "Instance": {
      "AppName": "MusicService"
    },
    "Client": {
      "EurekaServerServiceUrls": "http://localhost:8761/eureka"
    }
  },
  "Spring": {
    "Application": {
      "Name": "musicservice"
    },
    "Cloud": {
      "Config": {
        "Uri": "http://localhost:8888",
        "FailFast": true,
        "Enabled": true,
        "HealthEnabled": true,
        "Label": "musicservice",
        "DiscoveryEnabled": true,
        "DiscoveryServiceId": ""
      }
    }
  }
}
